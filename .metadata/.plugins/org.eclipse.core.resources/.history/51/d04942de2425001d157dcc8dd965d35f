package com.hdcc.controller;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;
import org.springframework.web.multipart.MultipartFile;

import com.hdcc.services.UploadPassportPictureService;

@RestController
@RequestMapping("/upload")
public class UploadPassportPictureController {
	
	private UploadPassportPictureService uploadPassportPictureService;
	@Autowired
	public UploadPassportPictureController(UploadPassportPictureService uploadPassportPictureService) {
		this.uploadPassportPictureService = uploadPassportPictureService;
	}
	public void uploadImage(MultipartFile file) {
		uploadPassportPictureService.uploadImage(file);
	}
	

}
